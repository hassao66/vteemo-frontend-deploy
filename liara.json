{
  "app": "vteemo",
  "platform": "static",
  "path": ".",
  "spa": true,
  "fallback": "index.html",
  "redirects": [
    {
      "source": "/*",
      "destination": "/index.html",
      "statusCode": 200
    }
  ],
  "headers": [
    {
      "source": "*.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "*.css,*.js,*.png,*.jpg,*.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}